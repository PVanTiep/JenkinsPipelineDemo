pipeline {
    agent any
    
    stages{
        stage('helo'){
            steps {
                echo 'helo world'
            }
        }
        
        stage('build'){
            steps{
                echo 'build'
            }
        }
        stage('test'){
            steps{
                echo 'build'
            }
        }
        stage('release'){
            
            steps{
                echo 'release'
            }
        }
        stage('exit'){
            
            steps{
                echo 'exit'
            }
        }
    }
}
